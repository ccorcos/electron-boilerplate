{
  "name": "electron-boilerplate",
  "version": "0.0.1",
  "description": "Electron boilerplate with TypeScript.",
  "scripts": {
    "setup": "mkdir -p build/renderer && cp -f src/renderer/index.html build/renderer/",
    "build": "npm run setup && tsc",
    "watch": "npm run setup && tsc --watch",
    "start": "electron build/main/main.js"
  },
  "repository": "https://github.com/ccorcos/electron-boilerplate",
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^12.12.32",
    "glamor": "^2.20.40",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "electron": "^8.2.0",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
