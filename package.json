    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.32",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.4.2",
    "electron": "^8.2.0",
    "file-loader": "^6.0.0",
